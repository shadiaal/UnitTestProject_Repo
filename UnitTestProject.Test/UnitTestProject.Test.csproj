    <PackageReference Include="NUnit3TestAdapter" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\UnitTestProject\UnitTestProject.csproj" />
  </ItemGroup>

</Project>
